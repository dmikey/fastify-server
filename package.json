{
  "name": "@xarc/fastify-server",
  "version": "2.1.0",
  "description": "A configurable Fastify web server",
  "main": "dist/electrode-server.js",
  "scripts": {
    "build": "tsc && npm run cpdir",
    "cpdir": "cp -r ./src/config  ./dist/config",
    "prepublishOnly": "xrun [[build], xarc/check]",
    "test": "npm run build && xrun xarc/test-only",
    "coverage": "npm run build && xrun xarc/test-cov",
    "sample": "node test/sample/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/electrode-io/fastify-server.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "next",
    "access": "public"
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "avoid",
    "trailingComma": "none"
  },
  "keywords": [
    "electrode",
    "fastify",
    "node",
    "webserver"
  ],
  "contributors": [
    {
      "name": "Joel Chen",
      "email": "xchen@walmartlabs.com"
    }
  ],
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 10.0.0"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "async-eventemitter": "^0.2.4",
    "chalk": "^4.1.0",
    "electrode-confippet": "^1.5.0",
    "fastify": "^3.1.1",
    "fastify-plugin": "^2.1.1",
    "lodash": "^4.17.15",
    "require-at": "^1.0.4",
    "xaa": "^1.6.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.15",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.4",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "@xarc/module-dev": "^2.2.4",
    "@xarc/run": "^1.0.2",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-walmart": "^2.2.1",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-jsdoc": "^21.0.0",
    "fastify-static": "^2.5.0",
    "intercept-stdout": "^0.1.2",
    "mitm": "^1.2.0",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "run-verify": "^1.2.1",
    "sinon": "^7.2.6",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.16",
    "superagent": "^5.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.9.7",
    "xstdout": "^0.1.1"
  },
  "nyc": {
    "all": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "*clap.js",
      "*clap.ts",
      "coverage",
      "src",
      "docs",
      "gulpfile.js",
      "test",
      "xrun*.js",
      "xrun*.ts"
    ],
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "cache": true,
    "extends": [
      "@istanbuljs/nyc-config-typescript"
    ]
  },
  "@xarc/module-dev": {
    "features": [
      "eslint",
      "eslintTS",
      "mocha",
      "typescript"
    ]
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register",
      "@xarc/module-dev/config/test/setup.js"
    ],
    "recursive": true
  }
}
